<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE gs-deployment SYSTEM "java://gs-deploy-desc.dtd">
<gs-deployment Name="PU ServiceBean">
    <Codebase>$[com.gigaspaces.system.codeserver]</Codebase>
    <!-- Declare attributes for the Hello example -->
    <ServiceBean Name="PU">

        <Interfaces>
            <Interface>org.openspaces.pu.container.servicegrid.PUServiceBean</Interface>
            <Resources>
                <JAR>puservicebean-dl.jar</JAR>
                <JAR>JSpaces.jar</JAR>
            </Resources>
        </Interfaces>
        <ImplementationClass>org.openspaces.pu.container.servicegrid.PUServiceBeanImpl
            <Resources>
                <JAR>puservicebean.jar</JAR>
            </Resources>
        </ImplementationClass>

        <!--
                <ServiceLevelAgreements>
                    <SLA ID="Swing" Low="10" High="80">
                        <PolicyHandler type="scaling" max="3"
                                       lowerDampener="3000" upperDampener="3000"/>
                    </SLA>
                </ServiceLevelAgreements>
        -->

        <Maintain>1</Maintain>
    </ServiceBean>

</gs-deployment>


