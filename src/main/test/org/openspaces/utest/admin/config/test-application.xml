<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:os-sla="http://www.openspaces.org/schema/sla"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:os-admin="http://www.openspaces.org/schema/admin"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd 
	                    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd
	                    http://www.openspaces.org/schema/admin http://www.openspaces.org/schema/9.0/admin/openspaces-admin.xsd">

	<context:annotation-config />

	<os-admin:application name="test-application">
		
		<os-admin:space name="test-space">
			<os-admin:depends-on name="test-pu" />
		</os-admin:space>
		
		<os-admin:pu processing-unit="test-pu.jar" />

	</os-admin:application>
</beans>