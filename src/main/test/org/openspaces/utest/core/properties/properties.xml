<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:os-core="http://www.openspaces.org/schema/core"
       xmlns:os-events="http://www.openspaces.org/schema/events"
       xmlns:os-remoting="http://www.openspaces.org/schema/remoting"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
       http://www.openspaces.org/schema/core http://www.openspaces.org/schema/9.0/core/openspaces-core.xsd
       http://www.openspaces.org/schema/events http://www.openspaces.org/schema/9.0/events/openspaces-events.xsd
       http://www.openspaces.org/schema/remoting http://www.openspaces.org/schema/9.0/remoting/openspaces-remoting.xsd">

    <os-core:annotation-support/>
    
    <bean id="beanLevelProperties" class="org.openspaces.core.properties.BeanLevelProperties">
    	<property name="contextProperties">
    		<props>
    			<prop key="key1">value1</prop>
    			<prop key="key2">value2</prop>
   			</props>
    	</property>
    </bean>
    
    <bean id="beanLevelPropertyPostProcessor" class="org.openspaces.core.properties.BeanLevelPropertyBeanPostProcessor">
    	<constructor-arg ref="beanLevelProperties"/>
    </bean>
    
    <bean id="beanLevelPropertiesBean" class="org.openspaces.utest.core.properties.BeanLevelPropertiesBean"/>
    
</beans>